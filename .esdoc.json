{
	"source": "./js",
	"destination": "./docs",
	"plugins": [
		{
			"name": "esdoc-standard-plugin",
			"option": {
				"test": {
					"source": "./test/",
					"interfaces": [
						"describe",
						"it",
						"context",
						"suite",
						"test"
					],
					"includes": [
						"(spec|Spec|test|Test)\\.js$"
					],
					"excludes": [
						"\\.config\\.js$"
					]
				},
				"manual": {
					"index": "./manual/index.md",
					"asset": "./manual/asset",
					"files": [
						"./manual/overview.md",
						"./manual/application_architecture.md",
						"./manual/tutorial.md",
						"./manual/benchmarking.md",
						"./CHANGELOG.md"
					]
				}
			}
		}
	]
}